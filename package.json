{
  "name": "file-uploader-component",
  "version": "1.1.5",
  "description": "A reactjs, angular and vuejs component of file uploader.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc -p src && tsc -p demo && webpack --config demo/webpack.config.js && npm run clean-rev && rev-static --config demo/rev-static.config.js",
    "lint": "tslint \"src/**/*.tsx\" \"demo/**/*.tsx\" \"src/**/*.ts\" \"demo/**/*.ts\" \"spec/**/*.ts\"",
    "clean-rev": "rimraf demo/react.bundle-*.js demo/vue.bundle-*.js demo/angular.bundle-*.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/file-uploader-component.git"
  },
  "keywords": [
    "file",
    "upload",
    "vue",
    "react",
    "angular2"
  ],
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/file-uploader-component/issues"
  },
  "homepage": "https://github.com/plantain-00/file-uploader-component#readme",
  "devDependencies": {
    "@angular/common": "4.1.0",
    "@angular/compiler": "4.1.0",
    "@angular/core": "4.1.0",
    "@angular/forms": "4.1.0",
    "@angular/http": "4.1.0",
    "@angular/platform-browser": "4.1.0",
    "@angular/platform-browser-dynamic": "4.1.0",
    "@angular/router": "4.1.0",
    "@angular/upgrade": "4.1.0",
    "@types/jasmine": "2.5.47",
    "@types/react": "15.0.23",
    "@types/react-dom": "15.5.0",
    "core-js": "2.4.1",
    "jasmine": "2.6.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "rev-static": "2.3.0",
    "rimraf": "2.6.1",
    "rxjs": "5.3.0",
    "tslint": "5.1.0",
    "typescript": "2.3.2",
    "vue": "2.3.0",
    "vue-class-component": "5.0.1",
    "webpack": "2.4.1",
    "zone.js": "0.8.9"
  },
  "dependencies": {
    "tslib": "1"
  }
